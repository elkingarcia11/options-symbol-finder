# Options Strike Finder Environment Variables
# Copy this file to .env and fill in your actual values

# Charles Schwab API Credentials
# Get these from: https://developer.schwab.com/
SCHWAB_APP_KEY=your_schwab_app_key_here
SCHWAB_APP_SECRET=your_schwab_app_secret_here

# Google Cloud Storage Configuration
# For token management and storage
GCS_BUCKET_NAME=your_gcs_bucket_name_here
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json

# Optional: Debug Mode
# Set to 'true' to enable debug output
DEBUG=false

# Optional: API Base URL (usually don't need to change)
# SCHWAB_API_BASE_URL=https://api.schwabapi.com

# Optional: Redirect URI (usually don't need to change)
# SCHWAB_REDIRECT_URI=https://127.0.0.1
